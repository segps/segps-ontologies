    <owl:ObjectProperty rdf:about="http://aseg.cs.concordia.ca/segps/ontologies/domain-specific/2015/02/securityDBs.owl#hasVulnerability">
        <rdfs:range rdf:resource="http://aseg.cs.concordia.ca/segps/ontologies/domain-specific/2015/02/securityDBs.owl#Vulnerability"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://se-on.org/ontologies/general/2012/02/main.owl#Product"/>
                    <rdf:Description rdf:about="http://se-on.org/ontologies/general/2012/02/main.owl#Release"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


