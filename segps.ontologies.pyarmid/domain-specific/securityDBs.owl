    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://aseg.cs.concordia.ca/segps/ontologies/domain-specific/2015/02/securityDBs.owl#affectProduct -->

    <owl:ObjectProperty rdf:about="http://aseg.cs.concordia.ca/segps/ontologies/domain-specific/2015/02/securityDBs.owl#affectProduct">
        <rdfs:range rdf:resource="http://aseg.cs.concordia.ca/segps/ontologies/domain-specific/2015/02/securityDBs.owl#AffectedProduct"/>
        <rdfs:domain rdf:resource="http://aseg.cs.concordia.ca/segps/ontologies/domain-specific/2015/02/securityDBs.owl#Vulnerability"/>
        <rdfs:subPropertyOf rdf:resource="http://aseg.cs.concordia.ca/segps/ontologies/domain-specific/2015/02/securityDBs.owl#hasAffect"/>
    </owl:ObjectProperty>
    


    <!-- http://aseg.cs.concordia.ca/segps/ontologies/domain-specific/2015/02/securityDBs.owl#affectRelease -->

    <owl:ObjectProperty rdf:about="http://aseg.cs.concordia.ca/segps/ontologies/domain-specific/2015/02/securityDBs.owl#affectRelease">
